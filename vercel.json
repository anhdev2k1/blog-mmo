{
    "version": 2,
    "builds": [
      {
        "src": "app/api/**/*.ts",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/$1" },
        { "src": "/(.*)", "dest": "/$1" }
      ]
      
  }
  